{
  "name": "@recall/vector-embedding-service",
  "version": "1.0.0",
  "description": "Vector Embedding Service for Recall - AI-powered semantic embeddings and similarity search",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "node --watch server.js",
    "test": "node --test test/**/*.test.js",
    "test:integration": "node --test test/integration/**/*.test.js",
    "test:performance": "node test/performance/benchmark.js",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "docker:build": "docker build -t recall/vector-embedding .",
    "docker:run": "docker run -p 3001:3001 recall/vector-embedding",
    "health": "curl http://localhost:3001/health/ready",
    "metrics": "curl http://localhost:3001/metrics",
    "test:embedding": "node test/embedding-test.js",
    "benchmark": "node test/performance/benchmark.js"
  },
  "keywords": [
    "embeddings",
    "vector",
    "similarity",
    "search",
    "openai",
    "faiss",
    "semantic",
    "ai",
    "microservice",
    "recall"
  ],
  "author": "Recall Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "redis": "^5.8.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "joi": "^17.11.0",
    "winston": "^3.11.0",
    "node-fetch": "^3.3.2",
    "faiss-node": "^0.5.0",
    "sharp": "^0.33.0",
    "crypto": "^1.0.1"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "prettier": "^3.1.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/n3wth/recall.git",
    "directory": "microservices-design/services/vector-embedding"
  },
  "bugs": {
    "url": "https://github.com/n3wth/recall/issues"
  },
  "homepage": "https://recall.newth.ai"
}