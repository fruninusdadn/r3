# r3call Environment Variables Template
# Copy this file to .env and fill in your actual values

# Mem0 API Key (required)
# Get your API key from: https://mem0.ai/dashboard/api-keys
MEM0_API_KEY=your_mem0_api_key_here

# Redis Connection URL (required)
# Default: redis://localhost:6379
REDIS_URL=redis://localhost:6379

# Smithery API Key (optional - for publishing to Smithery registry)
# Get your API key from: https://smithery.ai/dashboard
SMITHERY_API_KEY=your_smithery_api_key_here

# Debug Mode (optional)
# Set to "true" to enable debug logging
DEBUG=false

# User ID (optional)
# Default: system username
USER_ID=your_user_id_here